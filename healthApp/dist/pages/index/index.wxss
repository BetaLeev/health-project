page {
  background: #fcfcfc;
}
.nav-title {
  padding: 20rpx;
  text-align: center;
  box-sizing: border-box;
  width: 750rpx;
  background: #f1f2f4;
}
.list-wrap {
  width: 710rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  background-color: #f1f2f4;
  font-size: 26rpx;
  padding: 30rpx;
  box-sizing: border-box;
}
.list-wrap .list-title {
  text-align: center;
  font-size: 32rpx;
  font-weight: weight;
}
.list-wrap .list-content {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.list-wrap .list-img {
  width: 100%;
  height: 270rpx;
  margin-top: 5rpx;
}

